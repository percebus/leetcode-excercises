{
  "name": "leetcode-excercises",
  "version": "0.1.0",
  "description": "LeetCode.com excercises",
  "main": "Gruntfile.js",
  "scripts": {
    "pip:install:dev": "python -m pip install -r requirements.dev.txt",
    "pip:install:all": "python -m pip install -r requirements.txt",
    "npm:install:global": "npm install -g standard grunt-cli",
    "setup": "npm run pip:install:all && npm run npm:install:global",
    "setup:ci": "npm run setup",
    "grunt:lint": "grunt lint --color --verbose",
    "pylint": "python -m pylint --verbose --rcfile=.pylintrc",
    "pylint:problems": "npm run pylint -- problems/",
    "pylint:features": "npm run pylint -- features/",
    "lint:python": "npm run pylint:problems",
    "lint": "npm run lint:python",
    "aloe": "aloe --verbose",
    "pretest": "npm run lint",
    "test": "npm run aloe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/percebus/leetcode-excercises.git"
  },
  "keywords": [
    "leet",
    "leetcode",
    "l33t",
    "excercises"
  ],
  "author": "percebus",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/percebus/leetcode-excercises/issues"
  },
  "homepage": "https://github.com/percebus/leetcode-excercises#readme",
  "dependencies": {
    "grunt": "^1.4.1",
    "load-grunt-tasks": "^5.1.0"
  },
  "devDependencies": {
    "grunt-shell": "^3.0.1"
  }
}
